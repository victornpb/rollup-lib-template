{
  "name": "foo-bar",
  "nameFull": "FooBar",
  "version": "0.0.0",
  "description": "A foo-bar library",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/foo-bar.git"
  },
  "bugs": {
    "url": "https://github.com/username/foo-bar/issues"
  },
  "homepage": "https://github.com/username/foo-bar",
  "author": "username",
  "main": "dist/foo-bar.cjs.js",
  "module": "dist/foo-bar.esm.js",
  "unpkg": "dist/foo-bar.js",
  "globalVar": "FooBar",
  "scripts": {
    "start": "rollup -c -w",
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "lint": "eslint src/**/*.js",
    "lint:fix": "prettier-eslint src/**/*.js --write",
    "prebuild": "rimraf dist",
    "prepublishOnly": "npm run build && git add * && git commit -m \"Updating build\"",
    "release": "np patch --any-branch"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "foo",
    "bar"
  ],
  "sideEffects": false,
  "dependencies": {
    "core-js": "^3.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-private-methods": "^7.12.13",
    "@babel/plugin-transform-object-assign": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.17",
    "babel-plugin-array-includes": "^2.0.3",
    "eslint": "^7.20.0",
    "gatsby": "^2.1.18",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "np": "^4.0.2",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "rimraf": "^2.6.3",
    "rollup": "^2.39.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "tiny-dedent": "^1.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint:fix",
      "git add"
    ]
  }
}